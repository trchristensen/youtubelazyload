{
  "manifest": "1",
  "version": "1.0.1",
  "locale": {
  	"default": "en-us",
  	"supported": ["en-us"]
  },

  "elements": [
	  {
  	  "path": "files",
  	  "name": "Lazyload Youtube",
  	  "version": "1.0.1",
  	  "settings": {
  		"config": {},
  		  "autopop": true,
        "properties": [
          {
            "name": "settings",
            "label": "Settings",
            "type": "group",
            "properties": [
              {
                "name": "youtubeURL",
                "label": "",
                "type": "string",
                "default": "Youtube Video URL",
                "min": 1
              }
            ]
          }
        ]
      }
    }
  ]
}
